<?xml version="1.0" encoding="ISO-8859-1"?>
<?xml-stylesheet type="text/xsl" href="calendar.xslt"?>

<CALENDAR>
  <MONTH start="2" end="30">
    <!-- start is the day of the week that the month
starts. Wednesday in this case -->
    <EVENT date="4">
      <PARA>Some event description</PARA>
    </EVENT>
    <EVENT date="24">
      <PARA>another event description</PARA>
    </EVENT>
    <!--Lots of time for vacation this month! -->
    <TITLE>novembre 2004</TITLE>
  </MONTH>
  <MONTH start="4" end="31">
    <EVENT date="5">
      <PARA>Some event description</PARA>
      <PARA>Second para of description</PARA>
    </EVENT>
    <EVENT date="17">
      <PARA>first event description</PARA>
    </EVENT>
    <EVENT date="17">
      <PARA>second event description</PARA>
    </EVENT>
    <TITLE>décembre 2004</TITLE>
  </MONTH>
  <!--May be more MONTHs -->
</CALENDAR>
